<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.neobns.admin.flowcontrol.mapper.ConfigMapper">
    <select id="findByAppName" resultType="com.neobns.admin.flowcontrol.dto.ConfigDto">
        SELECT *
        FROM FLOW_CONFIG
        WHERE appName = #{appName}
        LIMIT 1 OFFSET 0
    </select>

</mapper>